<%= form_for @listing do |f| %>
	<%= f.label :title %>
	<%= f.text_field :title %>

	<%= f.label :description %>
	<%= f.text_field :description %>

	<%= f.label :all_tags %>
	<%= f.text_field :all_tags, placeholder:"Tags separated with comma" %>

	<%= f.submit %>
<%end%>

<%= link_to 'Home', welcome_pages_home_path %>